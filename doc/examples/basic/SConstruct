# -*-python-*-

import os

env = Environment(tools = ['toolchain'])
print(env['CXXFLAGS'])

VariantDir('build', 'src')
try:
  SConscript(os.path.join('build', 'cpp', 'SConscript'), exports="env")
except Exception as e:
  print e
  pass
try:
  SConscript(os.path.join('build', 'py', 'SConscript'), exports="env")
except Exception as e:
  print e
  pass